{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": [
		"src/index.ts",
		"src/utils/index.ts",
		"src/provider/index.ts",
		"src/jsonrpc/index.ts",
		"src/native/index.ts",
		"src/evm/index.ts",
		"src/primitives/Address/index.ts",
		"src/primitives/Hex/index.ts",
		"src/primitives/Uint/index.ts",
		"src/crypto/KZG/index.js"
	],
	"out": "docs/generated-api",
	"plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter"],
	"outputFileStrategy": "modules",
	"fileExtension": ".mdx",
	"entryFileName": "index",
	"frontmatterGlobals": {
		"description": "Auto-generated API documentation"
	},
	"indexFrontmatter": {
		"title": "Generated API Reference",
		"description": "Auto-generated TypeScript API documentation from source code"
	},
	"frontmatterCommentTags": ["description"],
	"exclude": [
		"**/*.test.*",
		"**/*.bench.*",
		"**/__mocks__/**",
		"**/examples/**",
		"**/docs/**",
		"dist/**",
		"types/**"
	],
	"excludeExternals": true,
	"excludePrivate": true,
	"excludeProtected": true,
	"categorizeByGroup": true,
	"readme": "none",
	"cleanOutputDir": true
}
