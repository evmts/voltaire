	"scripts": {
		"all": "pnpm install && pnpm build && pnpm lint && pnpm format && pnpm test:run",
		"bench": "pnpm dlx tsx scripts/run-benchmarks.ts",
		"bench:size": "size-limit",
		"build": "pnpm build:zig && pnpm build:dist && pnpm build:types",
		"build:zig": "zig build",
		"build:wasm": "zig build build-ts-wasm && zig build build-ts-wasm-fast",
		"build:dist": "NODE_OPTIONS=--max-old-space-size=8192 tsup",
		"build:types": "NODE_OPTIONS=--max-old-space-size=8192 tsup --dts-only && NODE_OPTIONS=--max-old-space-size=8192 tsc --emitDeclarationOnly --declaration",
		"clean": "rm -rf node_modules && rm -rf dist && rm -rf types && rm -rf zig-out && rm -rf zig-cache",
		"generate:jsonrpc": "zig build generate-jsonrpc",
		"clean:jsonrpc": "zig build clean-jsonrpc",
		"docs:dev": "cd docs && mint dev",
		"docs:playground:build": "cd playground && pnpm build:docs && rm -rf ../docs/public/playground && cp -R dist ../docs/public/playground && mkdir -p ../docs/public/node_modules/modern-monaco/dist && cp -R node_modules/modern-monaco/dist/* ../docs/public/node_modules/modern-monaco/dist/ && mkdir -p ../docs/public && cp node_modules/typescript/lib/typescript.js ../docs/public/typescript",
		"docs:build": "cd docs && mint build",
		"generate:docs": "echo 'Docs are pre-built. Use docs:build to build for production.'",
		"docs:api": "typedoc --options typedoc.json && npx tsx scripts/add-typedoc-frontmatter.ts",
		"mint:dev": "cd docs && mint dev",
		"mint:install": "npm install -g mintlify@latest",
		"format": "biome format . --write",
		"format:check": "biome format .",
		"lint": "biome check . --write --unsafe",
		"lint:check": "biome check . --verbose",
		"lint:deps": "pnpm dlx depcheck",
		"lint:package": "pnpm dlx publint --strict",
		"package:up": "pnpm up --latest",
		"size": "pnpm dlx tsx scripts/measure-bundle-sizes.ts",
		"test": "vitest --watch",
		"test:coverage": "vitest --coverage --run",
		"test:run": "vitest run",
		"test:native": "DYLD_LIBRARY_PATH=zig-out/lib vitest run src/typescript/native",
		"test:wasm": "vitest run src/wasm",
		"test:mcp": "vitest run -c vitest.mcp.config.ts",
		"test:examples": "pnpm dlx tsx test-examples.ts",
		"test:examples:zig": "zig run examples/test-examples.zig",
		"typecheck": "tsc --noEmit",
		"examples:generate": "pnpm dlx tsx scripts/generate-example-docs.ts",
		"examples:test": "pnpm dlx tsx scripts/validate-examples.ts",
		"examples:ci": "pnpm examples:test && pnpm examples:generate",
		"docs:ghost-tests": "node scripts/generate-ghost-tests.mjs",
		"predocs:dev": "npm run docs:ghost-tests",
		"predocs:build": "npm run docs:ghost-tests",
		"premint:dev": "npm run docs:ghost-tests",
		"predocs:playground:build": "npm run docs:ghost-tests",
		"prepublishOnly": "pnpm build && zig build crypto-wasm && pnpm lint:package && pnpm test:run"
	},
