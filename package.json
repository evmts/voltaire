	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "types/index.d.ts",
	"files": ["dist", "src", "!src/**/*.spec.ts", "!src/**/*.test.ts", "types"],
	"scripts": {
		"all": "bun i && bun run build && bun lint && bun format && bun test:run",
		"bench": "bun run scripts/run-benchmarks.ts",
		"bench:size": "size-limit",
		"build": "bun run build:zig && bun run build:dist && bun run build:types",
		"build:zig": "zig build",
		"build:wasm": "zig build build-ts-wasm && zig build build-ts-wasm-fast",
		"build:dist": "tsup",
		"build:types": "tsup --dts-only && tsc --emitDeclarationOnly --declaration",
		"clean": "rm -rf node_modules && rm -rf dist && rm -rf types && rm -rf zig-out && rm -rf zig-cache",
		"docs:dev": "astro dev",
		"docs:build": "astro build",
		"docs:preview": "astro preview",
		"format": "biome format . --write",
		"format:check": "biome format .",
		"lint": "biome check . --write --unsafe",
		"lint:check": "biome check . --verbose",
		"lint:deps": "bunx depcheck",
		"lint:package": "bunx publint --strict && bunx attw --pack",
		"package:up": "bun up --latest",
		"size": "bun run scripts/measure-bundle-sizes.ts",
		"test": "vitest --watch",
		"test:coverage": "vitest --coverage --run",
		"test:run": "vitest run",
		"test:native": "DYLD_LIBRARY_PATH=zig-out/lib vitest run src/typescript/native",
		"test:wasm": "vitest run src/wasm",
		"typecheck": "tsc --noEmit"
	},
